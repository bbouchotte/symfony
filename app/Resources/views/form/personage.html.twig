{% block form_row %}
{% spaceless %}
    <div class="form_row">
		{{ form_start(form, {'attr': {'novalidate': 'novalidate', 'class': 'form_control' }}) }}
        {{ form_label(form) }}
        {{ form_errors(form) }}
        {{ form_widget(form) }}
       	{{ form_end(form) }}
        
    </div>
{% endspaceless %}
{% endblock form_row %}